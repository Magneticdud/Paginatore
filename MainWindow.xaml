<Window x:Class="Paginatore.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Paginatore"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Slider x:Name="sliderpart" VerticalAlignment="Top" Margin="15,30,15,0" Value="6" SmallChange="1" Minimum="1" Maximum="8"/>
        <Slider x:Name="slidermax" VerticalAlignment="Top" Margin="15,60,15,0" Value="50" SmallChange="1" Maximum="100" Minimum="1"/>
        <Button x:Name="buttongo" Content="Go" HorizontalAlignment="Center" Margin="0,100,0,0" VerticalAlignment="Top" Width="75" Click="buttongo_Click"/>
        <TextBox x:Name="textBoxoutput" Margin="25,130,22,25" TextWrapping="Wrap" Text="TextBox"/>

    </Grid>
</Window>
